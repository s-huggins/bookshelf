{
  "name": "goodreads-clone",
  "version": "1.0.0",
  "description": "A simplified MERN clone of goodreads",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install && cd..",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'green,blue' \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "author": "Stuart Huggins",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.1",
    "cors": "^2.8.5",
    "debounce-async": "0.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.1",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "validator": "^12.1.0",
    "xml2js": "^0.4.23",
    "xss-clean": "^0.1.1"
  }
}
